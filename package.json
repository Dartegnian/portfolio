{
  "name": "dartegnians-portfolio",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --disable-host-check --host 0.0.0.0",
    "start": "ng serve --disable-host-check --host 0.0.0.0",
    "generate-images": "npx gulp compress",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run dartegnians-portfolio:serve-ssr",
    "serve:ssr": "node dist/dartegnians-portfolio/server/main.js",
    "build:ssr": "ng run dartegnians-portfolio:prerender && ng run dartegnians-portfolio:server",
    "serve:ssr:dartegnians-portfolio": "node dist/dartegnians-portfolio/server/server.mjs",
    "prerender": "ng run dartegnians-portfolio:prerender",
    "scully-build": "ng build --configuration production && npx scully --noPrompt --project dartegnians-portfolio --scanRoutes",
    "local": "ng serve --disable-host-check --host 0.0.0.0",
    "local-gulp": "npx gulp compress && ng serve --disable-host-check",
    "docker-build-dev": "docker build -t dartegnians-portfolio-dev -f ./docker/Dockerfile.dev .",
    "docker-run-dev": "docker-compose -f docker/docker-compose.dev.yml up",
    "docker-build-prod": "docker build -t dartegnians-portfolio -f docker/Dockerfile.prod .",
    "docker-run-prod": "docker-compose -f docker/docker-compose.prod.yml down && docker-compose -f docker/docker-compose.prod.yml up"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.8",
    "@angular/common": "^19.2.8",
    "@angular/compiler": "^19.2.8",
    "@angular/core": "^19.2.8",
    "@angular/forms": "^19.2.8",
    "@angular/platform-browser": "^19.2.8",
    "@angular/platform-browser-dynamic": "^19.2.8",
    "@angular/platform-server": "^19.2.8",
    "@angular/router": "^19.2.8",
    "@angular/service-worker": "^19.2.8",
    "@angular/ssr": "^19.2.9",
    "@material/material-color-utilities": "^0.3.0",
    "bootstrap": "^5.3.5",
    "express": "^4.21.2",
    "idb": "^8.0.2",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.9",
    "@angular/cli": "^19.2.9",
    "@angular/compiler-cli": "^19.2.8",
    "@types/express": "^5.0.1",
    "@types/jasmine": "^5.1.7",
    "@types/node": "^22.15.2",
    "gulp": "^4.0.2",
    "gulp-sharp-responsive": "^0.4.1",
    "jasmine-core": "^5.6.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "typescript": "~5.6.3"
  }
}
